
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0" />
    <title>Home</title>
    <!--CSS-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link href="~/Content/materialize-v0.97.5/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />

</head>


<body onload="nobackbutton()">
    <!--Start Nav bar -->
    <nav class="light-blue lighten-1" role="navigation">
        <div class="nav-wrapper container">
            <a href="#!" class="brand-logo">  <i class="material-icons">cloud</i> </a>
            <ul class="right hide-on-med-and-down">
                <li><a href="#" data-activates="slide-out" class="button-collapseUser"><i class="material-icons">person_pin</i></a></li>
                <li><a href="mobile.html"><i class="material-icons">info_outline</i></a></li>
                <li><a href="#" onclick="closeSession()"><i class="material-icons">lock</i></a></li>
            </ul>
            <ul id="nav-mobile" class="side-nav">

                <li><a href="#" data-activates="slide-out" class="button-collapseUser">Perfil</a></li>
                <li><a href="#">Información</a></li>

            </ul>
            <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
    </nav>
    <!--End Nav bar-->
    <!--Nav bar slider -->
    <ul id="slide-out" class="side-nav">
        <li>
            <div class="userView">
                <img class="background responsive-img" src="~/Img/logoSinapsis.png">
                <a href="#!user"><img class="circle" src="~/Img/foro_diego.png"></a>
                <a href="#!name"><span class="white-text name" id="nameUser">John Doe</span></a>
                <a href="#!email"><span class="white-text email" id="emailUser">jdandturk@gmail.com</span></a>
                <a href="#!email"><span class="white-text " id="role">jdandturk@gmail.com</span></a>
            </div>
        </li>
        <li><div class="divider"></div></li>
        <li><a href="#" id="phoneUser"></a></li>
        <li><a href="#" id="phoneUser2"></a></li>
        <li><div class="divider"></div></li>
        <li><a href="#" id="celUser"></a></li>
        <li><a href="#" id="celUser2"></a></li>
        <li><div class="divider"></div></li>

    </ul>
    <!--End Nav bar slider -->
    <!--Start Container-->
    <div class="container">
        <div class="section">
            <!--   Icon Section   -->
            <div class="row">
                <div class="col s12 m6 l4" id="Business" onclick="selctView(this.id)">
                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">flash_on</i>
                        </h2>
                        <h5 class="center">Empresa</h5>

                    </div>
                </div>
                <div class="col s12 m6 l4" id="Client" onclick="selctView(this.id)">
                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">group</i>
                        </h2>
                        <h5 class="center">Cliente</h5>

                    </div>
                </div>
                <div class="col s12 m6 l4" id="Employee" onclick="selctView(this.id)">

                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">settings</i>
                        </h2>
                        <h5 class="center">Empleado</h5>

                    </div>
                </div>

                <div class="col s12 m6 l4" id="Visit" onclick="selctView(this.id)">
                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">flash_on</i>
                        </h2>
                        <h5 class="center">Visitas</h5>

                    </div>
                </div>
                <div class="col s12 m6 l4" id="Quotes" onclick="selctView(this.id)">
                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">group</i>
                        </h2>
                        <h5 class="center">Cotizaciones</h5>

                    </div>
                </div>
                <div class="col s12 m6 l4" id="Reports" onclick="selctView(this.id)">
                    <div class="icon-block">
                        <h2 class="center light-blue-text">
                            <i class="large material-icons">group</i>
                        </h2>
                        <h5 class="center">Reportes</h5>

                    </div>
                </div>

            </div>
        </div>
      
    </div>
    <!--End Start Container-->
    <!--Scripts 
      Order Script 
        1. Jquery
        2. Materializr
        3. Function
        4. Function Local
      -->
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Content/materialize-v0.97.5/js/materialize.js"></script>
    <script src="~/Scripts/jsProject/Functions.js"></script>
    <script src="~/Scripts/jsProject/CtrHome.js"></script>


</body>
</html>
